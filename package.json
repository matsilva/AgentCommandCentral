{
	"name": "acc",
	"version": "0.1.0",
	"description": "Various CLI utilities for agent commands",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "bun build src/index.ts --compile --outfile=acc",
		"test": "bun test",
		"fmt": "bunx biome format --write",
		"lint": "bunx biome lint --write",
		"check": "bunx biome check --write"
	},
	"bin": {
		"acc": "./src/index.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/bun": "latest",
		"@voltagent/cli": "^0.1.10",
		"typescript": "^5"
	},
	"dependencies": {
		"@ai-sdk/openai": "^2",
		"@voltagent/core": "^1.1.15",
		"@voltagent/libsql": "^1.0.6",
		"@voltagent/logger": "^1.0.2",
		"@voltagent/server-hono": "^1.0.7",
		"ai": "^5.0.48",
		"chalk": "^5.3.0",
		"commander": "^12.0.0",
		"zod": "^4.1.11"
	}
}
